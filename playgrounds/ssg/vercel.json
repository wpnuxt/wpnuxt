{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm --filter @wpnuxt/core build && nuxi generate",
  "outputDirectory": ".output/public",
  "trailingSlash": true,
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1536, 1920, 2048, 3840],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "wordpress.wpnuxt.com"
      }
    ]
  }
}
