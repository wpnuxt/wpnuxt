{
  "name": "wpnuxt",
  "version": "2.0.0-beta.3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run dev:prepare && pnpm -r --if-present build",
    "dev:prepare": "pnpm -r --if-present dev:prepare",
    "dev:core": "pnpm --filter @wpnuxt/playground-core dev",
    "dev:blocks": "pnpm --filter @wpnuxt/playground-blocks dev",
    "dev:full": "pnpm --filter @wpnuxt/playground-full dev",
    "dev:ssg": "pnpm --filter @wpnuxt/playground-ssg dev",
    "dev:docs": "pnpm --filter @wpnuxt/docs dev",
    "dev": "pnpm dev:full",
    "lint": "eslint .",
    "test": "pnpm -r --if-present test",
    "test:e2e": "cd e2e && node run-tests.mjs",
    "typecheck": "pnpm -r --if-present typecheck",
    "check": "pnpm run dev:prepare && pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.15.1",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.5",
    "eslint": "^10.0.0",
    "release-it": "^19.2.4"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
