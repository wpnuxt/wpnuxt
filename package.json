{
  "name": "wpnuxt",
  "version": "1.0.0",
  "description": "WPNuxt",
  "repository": "wpnuxt/wpnuxt",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:basic": "nuxi dev playground-basic",
    "dev:basic:build": "nuxi build playground-basic",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground && nuxi prepare playground-basic",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "4.0.0-rc.0",
    "@radya/nuxt-dompurify": "^1.0.5",
    "defu": "^6.1.4",
    "graphql": "^16.11.0",
    "nuxt-graphql-middleware": "5.2.0",
    "scule": "^1.3.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.5.2",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.6",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "^24.0.13",
    "changelogen": "^0.6.2",
    "eslint": "^9.31.0",
    "nuxt": "4.0.0-rc.0",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.1"
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971"
}
