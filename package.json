{
  "name": "wpnuxt",
  "version": "2.0.0-alpha.13",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm --filter './packages/**' dev:prepare && pnpm --filter './packages/**' build && pnpm --filter './playgrounds/**' build",
    "dev:prepare": "pnpm --filter './packages/**' dev:prepare && pnpm --filter './playgrounds/**' nuxt:prepare",
    "dev:core": "pnpm --filter @wpnuxt/playground-core dev",
    "dev:blocks": "pnpm --filter @wpnuxt/playground-blocks dev",
    "dev:full": "pnpm --filter @wpnuxt/playground-full dev",
    "dev:ssg": "pnpm --filter @wpnuxt/playground-ssg dev",
    "dev:docs": "pnpm --filter @wpnuxt/docs dev",
    "dev": "pnpm dev:full",
    "lint": "eslint .",
    "test": "pnpm --filter './packages/**' test",
    "typecheck": "pnpm --filter './**' typecheck",
    "clean": "pnpm -r run clean && rm -rf node_modules",
    "check": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build",
    "check:full": "pnpm clean && pnpm install && pnpm run dev:prepare && pnpm run check",
    "release": "release-it"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.14.0",
    "@release-it/bumper": "^7.0.5",
    "@release-it/conventional-changelog": "^10.0.5",
    "eslint": "^10.0.0",
    "release-it": "^19.2.4"
  },
  "packageManager": "pnpm@10.28.2",
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
