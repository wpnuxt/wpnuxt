{
  "name": "@wpnuxt/blocks",
  "version": "2.0.0-alpha.9",
  "description": "Vue components for rendering WordPress Gutenberg blocks in Nuxt",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "wordpress",
    "gutenberg",
    "blocks",
    "vue",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wpnuxt/wpnuxt.git",
    "directory": "packages/blocks"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "dev:prepare": "nuxt-module-build build --stub",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "vue-tsc --noEmit",
    "clean": "rm -rf dist .nuxt node_modules"
  },
  "dependencies": {
    "@nuxt/image": "^2.0.0",
    "@nuxt/kit": "4.3.0",
    "@radya/nuxt-dompurify": "^1.0.5",
    "scule": "^1.3.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "4.3.0",
    "@nuxt/test-utils": "^3.23.0",
    "@types/node": "^25.2.1",
    "nuxt": "4.3.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^3.2.3"
  },
  "peerDependencies": {
    "@nuxt/ui": "^4.0.0",
    "nuxt": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/ui": {
      "optional": true
    }
  }
}
