{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "declaration": false,
    "types": ["node"],
    "paths": {
      "#imports": ["./src/runtime/types/stub.d.ts"],
      "nuxt/schema": ["./src/runtime/types/nuxt-schema.d.ts"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["dist", "node_modules", "test", "app"]
}
